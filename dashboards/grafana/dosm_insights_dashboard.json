{
  "title": "DOSM Insights \u2013 API",
  "timezone": "browser",
  "panels": [
    {
      "type": "stat",
      "title": "Requests (total)",
      "id": 1,
      "targets": [
        {"expr": "sum(http_requests_total)"}
      ]
    },
    {
      "type": "stat",
      "title": "p95 latency (ms)",
      "id": 2,
      "targets": [
        {"expr": "histogram_quantile(0.95, sum by (le) (rate(http_request_latency_ms_bucket[5m])))"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests per second",
      "id": 3,
      "targets": [
        {"expr": "sum(rate(http_requests_total[1m]))"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Error rate (%)",
      "id": 4,
      "targets": [
        {"expr": "(sum(rate(http_requests_total{status=\"500\"}[5m])) / sum(rate(http_requests_total[5m]))) * 100"}
      ]
    },
    {
      "type": "timeseries",
      "title": "RAG decisions per minute",
      "id": 5,
      "targets": [
        {"expr": "sum by (decision) (rate(rag_decisions_total[1m]))"}
      ],
      "legend": {"show": true}
    },
    {
      "type": "stat",
      "title": "Refusal rate (%)",
      "id": 6,
      "targets": [
        {"expr": "(sum(rate(rag_decisions_total{decision=\"refuse\"}[5m])) / sum(rate(rag_decisions_total[5m]))) * 100"}
      ]
    },
    {
      "type": "stat",
      "title": "Clarify ratio (%)",
      "id": 7,
      "targets": [
        {"expr": "(sum(rate(rag_decisions_total{decision=\"clarify\"}[5m])) / sum(rate(rag_decisions_total[5m]))) * 100"}
      ]
    }
  ],
  "version": 1
}